#include <stdio.h>

float calcularAreaRetangulo(float base, float altura) {
    return base * altura;
}

int main() {
    float base, altura;

    printf("Digite a base e a altura do retângulo: ");
    scanf("%f %f", &base, &altura);

    float area = calcularAreaRetangulo(base, altura);
    printf("Área do retângulo: %.2f\n", area);

    return 0;
}
